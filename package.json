{
  "name": "bioengine-angular",
  "version": "0.0.0",
  "scripts": {
    "generate-barrels": "barrelsby --delete -d lib/src -n public-api",
    "build": "npm run generate-barrels && ng build bioengine-angular && scss-bundle -e ./lib/src/styles.scss -d ./dist/styles.scss",
    "watch": "npm run generate-barrels && ng build --watch bioengine-angular",
    "release": "npm run build && npm publish dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.5",
    "@angular/common": "8.2.5",
    "@angular/compiler": "8.2.5",
    "@angular/core": "8.2.5",
    "@angular/forms": "8.2.5",
    "@angular/platform-browser": "8.2.5",
    "@angular/platform-browser-dynamic": "8.2.5",
    "@angular/router": "8.2.5",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.3",
    "@angular-devkit/build-ng-packagr": "0.803.3",
    "@angular/cdk": "8.1.4",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "8.2.5",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/language-service": "8.2.5",
    "@angular/material": "8.1.4",
    "@angular/material-moment-adapter": "8.1.4",
    "@ckeditor/ckeditor5-angular": "1.1.0",
    "@ckeditor/ckeditor5-build-balloon": "12.4.0",
    "@types/node": "12.7.4",
    "barrelsby": "2.1.1",
    "codelyzer": "5.1.0",
    "moment": "2.24.0",
    "ng-packagr": "5.5.0",
    "ngx-custom-validators": "8.0.0",
    "ngx-input-file": "8.0.1",
    "ngx-moment": "3.4.0",
    "object-path": "0.11.4",
    "protractor": "5.4.2",
    "scss-bundle": "^3.0.1",
    "ts-node": "8.3.0",
    "tsickle": "0.37.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3",
    "url-parse": "1.4.7",
