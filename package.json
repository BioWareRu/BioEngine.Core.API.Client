{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "bioengine.core.api.client",
  "version": "1.0.8",
  "description": "Core angular components for api client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run generate-barrels && ng-packagr -p package.json",
    "publish": "npm publish dist",
    "generate-barrels": "barrelsby --delete -d src -n public_api",
    "release": "npm run build && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BioWareRu/BioEngine.Core.Api.Client.git"
  },
  "author": "George Drak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BioWareRu/BioEngine.Core.Api.Client/issues"
  },
  "homepage": "https://github.com/BioWareRu/BioEngine.Core.Api.Client#readme",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "."
    ]
  },
  "dependencies": {
    "@angular/animations": "8.0.3",
    "@angular/cdk": "8.0.1",
    "@angular/common": "8.0.3",
    "@angular/compiler": "8.0.3",
    "@angular/core": "8.0.3",
    "@angular/elements": "8.0.3",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.0.3",
    "@angular/material": "8.0.1",
    "@angular/material-moment-adapter": "8.0.1",
    "@angular/platform-browser": "8.0.3",
    "@angular/platform-browser-dynamic": "8.0.3",
    "@ckeditor/ckeditor5-angular": "1.1.0",
    "@ckeditor/ckeditor5-build-balloon": "12.2.0",
    "@fortawesome/fontawesome-free": "5.9.0",
    "document-register-element": "1.13.2",
    "moment": "2.24.0",
    "ngx-custom-validators": "8.0.0",
    "ngx-input-file": "8.0.0",
    "ngx-moment": "3.4.0",
    "object-path": "0.11.4",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "url-parse": "^1.4.7",
    "uuid": "3.3.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^8.0.3",
    "@types/node": "12.0.10",
    "@types/twitter-for-web": "0.0.1",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "3.4.4",
    "barrelsby": "^2.1.1",
    "codelyzer": "5.1.0",
    "ng-packagr": "^5.3.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-consistent-codestyle": "1.15.1",
    "typescript": "3.4.5"
  }
}
